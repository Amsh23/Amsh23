
{
    "version": "2.0.0",
    "tasks": [
      {
        "type": "shell",
        "label": "C Compiler (GCC)",
        "command": "gcc",
        "args": [
          "-g",
          "${file}",
          "-o",
          "${fileDirname}\\${fileBasenameNoExtension}.exe"
        ],
        "options": {
          "cwd": "${workspaceFolder}"
        }
      }
    ]
  }
  